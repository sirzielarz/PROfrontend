  "version": "0.2.0",
  "configurations": [
    {
      "type": "firefox",
      "request": "attach",
      "name": "Attach"
    },
    {
      "type": "firefox",
      "request": "attach",
      "name": "Attach FFFFFFF"
    },
    {
      "name": "Launch",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "file": "${workspaceFolder}/s"
    },
    {
      "name": "Launch localhost FF",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "pathMappings": [
        {
          "url": "file:///C:/Users/XX/dev/PROfrontend/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    },
    {
      "name": "Attach",
      "type": "firefox",
      "request": "attach"
    },
    {
      "name": "Launch WebExtension",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "addonPath": "${workspaceFolder}"
    }
  ]
}
